	title = {Attack Some while Protecting Others: Selective Attack Strategies for
                  Attacking and Protecting Multiple Concepts},
	booktitle = {Proceedings of the 2023 {ACM} {SIGSAC} Conference on Computer and
                  Communications Security, {CCS} 2023, Copenhagen, Denmark, November
                  26-30, 2023},
	pages = {801--814},
