                  Christian Damsgaard Jensen and
                  Cas Cremers and
                  Engin Kirda},
	title = {Understanding and Detecting Abused Image Hosting Modules as Malicious
                  Services},
	booktitle = {Proceedings of the 2023 {ACM} {SIGSAC} Conference on Computer and
                  Communications Security, {CCS} 2023, Copenhagen, Denmark, November
                  26-30, 2023},
	pages = {3213--3227},
	publisher = {{ACM}},
	year = {2023},
	url = {https://doi.org/10.1145/3576915.3623143},
	doi = {10.1145/3576915.3623143},
	timestamp = {Mon, 04 Dec 2023 13:56:31 +0100},
	biburl = {https://dblp.org/rec/conf/ccs/HongWCLYY23.bib},
	bibsource = {dblp computer science bibliography, https://dblp.org},
	abstract = {As a new type of underground ecosystem, the exploitation of Abused IHMs as MalIcious sErvices (AIMIEs) is becoming increasingly prevalent among miscreants to host illegal images and propagate harmful content. However, there has been little effort to understand this new menace, in terms of its magnitude, impact, and techniques, not to mention any serious effort to detect vulnerable image hosting modules on a large scale. To fulfill this gap, this paper presents the first measurement study of AIMIEs. By collecting and analyzing 89 open-sourced AIMIEs, we reveal the landscape of AIMIEs, report the evolution and evasiveness of abused image hosting APIs from reputable companies such as Alibaba, Tencent, and Bytedance, and identify real-world abused images uploaded through those AIMIEs. In addition, we propose a tool, called Viola, to detect vulnerable image hosting modules (IHMs) in the wild. We find 477 vulnerable IHM upload APIs associated with 338 web services, which integrated vulnerable IHMs, and 207 victim FQDNs. The highest-ranked domain with vulnerable web service is baidu.com, followed by bilibili.com and 163.com. We have reported abused and vulnerable IHM upload APIs and received acknowledgments from 69 of them by the time of paper submission.}
}


@inproceedings{DBLP:conf/ccs/AranhaHST23,
	author = {Diego F. Aranha and
                  Benjamin Salling Hvass and
                  Bas Spitters and
                  Mehdi Tibouchi},
	editor = {Weizhi Meng and
                  Christian Damsgaard Jensen and
                  Cas Cremers and
                  Engin Kirda},
	title = {Faster Constant-time Evaluation of the Kronecker Symbol with Application
                  to Elliptic Curve Hashing},
	booktitle = {Proceedings of the 2023 {ACM} {SIGSAC} Conference on Computer and
                  Communications Security, {CCS} 2023, Copenhagen, Denmark, November
                  26-30, 2023},
	pages = {3228--3238},
	publisher = {{ACM}},
	year = {2023},
	url = {https://doi.org/10.1145/3576915.3616597},
	doi = {10.1145/3576915.3616597},
	timestamp = {Tue, 28 Nov 2023 20:06:17 +0100},
	biburl = {https://dblp.org/rec/conf/ccs/AranhaHST23.bib},
	bibsource = {dblp computer science bibliography, https://dblp.org},
	abstract = {We generalize the Bernstein-Yang (BY) algorithm [11] for constant-time modular inversion to compute the Kronecker symbol, of which the Jacobi and Legendre symbols are special cases. We first develop a basic and easy-to-implement algorithm, defined with full-precision division steps. We then describe an optimized version due to Hamburg [21] over word-sized inputs, and formally verify its correctness. Along the way, we introduce a number of optimizations for implementing both versions in constant time. The resulting algorithms are particularly suitable for computing the Legendre symbol with dense prime p, where no efficient addition chain is known for exponentiating to p-1 over 2, as it is often the case in pairing-friendly elliptic curves. Our high-speed implementation for a range of parameters shows that the new algorithm is up to 40 times faster than exponentiation, and up to 25.7% faster than the previous state of the art. We illustrate our techniques with hashing to elliptic curves using the SwiftEC algorithm [17], with savings of 14.7%-48.1%, and to accelerating the CTIDH isogeny-based key exchange [7], with savings of 3.5-13.5%.}
}


@inproceedings{DBLP:conf/ccs/Fischlin023,
