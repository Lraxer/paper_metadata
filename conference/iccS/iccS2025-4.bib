	bibsource = {dblp computer science bibliography, https://dblp.org},
	abstract = {This paper presents an algorithm designed to eliminate the direct evaluation of both strongly and weakly singular boundary integrals in the Parametric Integral Equation System (PIES) for the analysis of three-dimensional multidomain orthotropic problems. The proposed method regularizes PIES by incorporating an auxiliary regularization functions with coefficients that effectively remove singularities. Consequently, the regularized PIES eliminates the need to explicitly evaluate singular integrals, enabling all integrals to be computed using standard Gaussian quadrature.}
}


@inproceedings{DBLP:conf/iccS/XieDWLWHSZYT25,
	author = {Jie Xie and
