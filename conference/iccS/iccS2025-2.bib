@inproceedings{DBLP:conf/iccS/SantraBC25,
	author = {Abhishek Santra and
                  Sanjukta Bhowmick and
                  Sharma Chakravarthy},
	editor = {Michael H. Lees and
                  Wentong Cai and
                  Siew Ann Cheong and
                  Yi Su and
                  David Abramson and
                  Jack J. Dongarra and
                  Peter M. A. Sloot},
	title = {CommPlex: Community in MultiPlexes - Definition and a Suite of Algorithms
                  for Analysis},
	booktitle = {Computational Science - {ICCS} 2025 - 25th International Conference,
                  Singapore, July 7-9, 2025, Proceedings, Part {II}},
	series = {Lecture Notes in Computer Science},
	volume = {15904},
	pages = {3--18},
	publisher = {Springer},
	year = {2025},
	url = {https://doi.org/10.1007/978-3-031-97629-2\_1},
	doi = {10.1007/978-3-031-97629-2\_1},
	timestamp = {Thu, 17 Jul 2025 13:55:42 +0200},
	biburl = {https://dblp.org/rec/conf/iccS/SantraBC25.bib},
	bibsource = {dblp computer science bibliography, https://dblp.org},
	abstract = {Multiplexes (also termed Multilayer Networks or networks of networks) are useful for modeling data sets with multiple entity types, and relationships among them. The notion of\xa0a community is well-defined for simple graphs (or a monoplex/network) and is widely used for aggregate analysis on graphs. Several simple graph algorithms (e.g., Infomap, Louvain) for computing a community and algorithms for computing other metrics (e.g., centrality, substructure, etc.) exist as well. Although multilayer networks (MLNs) are used for modeling, the concept of a community\xa0and algorithms for its computation are lacking. Ideally, an\xa0MLN community definition should be comparable to the simple\xa0graph definition and be a generalization. As MLNs have structure in\xa0terms of layers, including inter-layer edges, it is important to define\xa0a community that includes its structure and semantics. The resulting community should also be an MLN. The focus of this paper is\xa0on heterogeneous MLN (or HeMLN), which is a type of MLN with explicitly defined inter-layer edges. In this paper, we introduce a community definition\xa0for HeMLNs that is structure-preserving and is also consistent with\xa0the traditional definition. Layer semantics are also preserved\xa0for drill-down and visualization. First, we define a community for\xa0any k connected layers of a HeMLN (termed k-community (1-community is the same as\xa0the traditional community on a simple graph or a layer of HeMLN.))\xa0using binary composition. Then, we propose an algorithm for\xa0its computation using the concept of bipartite graphs. Further, we\xa0show how weight metrics can be customized to include the semantics\xa0of participating community characteristics. Our definition:\xa0i) leverages extant simple graph community computation algorithms,\xa0ii) composes partial results from different layers for computing\xa0HeMLN communities (i.e., uses the decoupling approach), iii)\xa0is customizable using weight metrics based on participating communities, and iv) is computationally efficient. We\xa0have experimentally validated the community concept (definition\xa0and computation) on several real-world and synthetic data sets.}
}


@inproceedings{DBLP:conf/iccS/VenkateshMAKD25,
	author = {Supreeth Mysore Venkatesh and
                  Antonio Macaluso and
                  Diego Arenas and
                  Matthias Klusch and
                  Andreas Dengel},
	editor = {Michael H. Lees and
                  Wentong Cai and
                  Siew Ann Cheong and
                  Yi Su and
                  David Abramson and
                  Jack J. Dongarra and
                  Peter M. A. Sloot},
	title = {i-QLS: Quantum-Supported Algorithm for Least Squares Optimization
                  in Non-linear Regression},
	booktitle = {Computational Science - {ICCS} 2025 - 25th International Conference,
                  Singapore, July 7-9, 2025, Proceedings, Part {II}},
	series = {Lecture Notes in Computer Science},
	volume = {15904},
	pages = {19--34},
	publisher = {Springer},
	year = {2025},
	url = {https://doi.org/10.1007/978-3-031-97629-2\_2},
	doi = {10.1007/978-3-031-97629-2\_2},
	timestamp = {Sat, 09 Aug 2025 12:14:13 +0200},
	biburl = {https://dblp.org/rec/conf/iccS/VenkateshMAKD25.bib},
	bibsource = {dblp computer science bibliography, https://dblp.org},
	abstract = {We propose an iterative quantum-assisted least squares (i-QLS) optimization method that leverages quantum annealing\xa0to overcome the scalability and precision limitations of prior quantum least squares approaches. Unlike traditional QUBO-based formulations, which suffer from an exponential qubit overhead due\xa0to fixed discretization, our approach refines the solution\xa0space iteratively, enabling exponential convergence while maintaining\xa0a constant qubit requirement per iteration. This iterative refinement transforms the problem into an anytime algorithm, allowing\xa0for flexible computational trade-offs. Furthermore, we extend\xa0our framework beyond linear regression to non-linear function approximation via spline-based modeling, demonstrating\xa0its adaptability to complex regression tasks. We empirically validate i-QLS on the D-Wave quantum annealer, showing that our method efficiently scales to high-dimensional problems, achieving competitive accuracy with classical solvers while outperforming prior quantum approaches. Experiments confirm that i-QLS enables near-term quantum hardware to perform regression tasks with improved precision and scalability, paving the way for practical quantum-assisted machine learning applications.}
}


@inproceedings{DBLP:conf/iccS/AnsoteguiRSX25,
	author = {Carlos Ans{\'{o}}tegui and
                  Vaidyanathan Peruvemba Ramaswamy and
                  Stefan Szeider and
                  Hai Xia},
	editor = {Michael H. Lees and
                  Wentong Cai and
                  Siew Ann Cheong and
                  Yi Su and
                  David Abramson and
                  Jack J. Dongarra and
                  Peter M. A. Sloot},
	title = {Uncovering and Verifying Optimal Community Structure in Complex Networks:
                  {A} MaxSAT Approach},
	booktitle = {Computational Science - {ICCS} 2025 - 25th International Conference,
                  Singapore, July 7-9, 2025, Proceedings, Part {II}},
	series = {Lecture Notes in Computer Science},
	volume = {15904},
	pages = {35--49},
	publisher = {Springer},
	year = {2025},
	url = {https://doi.org/10.1007/978-3-031-97629-2\_3},
	doi = {10.1007/978-3-031-97629-2\_3},
	timestamp = {Tue, 05 Aug 2025 22:39:46 +0200},
	biburl = {https://dblp.org/rec/conf/iccS/AnsoteguiRSX25.bib},
	bibsource = {dblp computer science bibliography, https://dblp.org},
	abstract = {Network modularity is central to understanding phenomena in diverse domains, from biology and social science to engineering\xa0and computational physics. However, computing the optimal modularity—an NP-hard measure quantifying community strength—has remained computationally intractable at large scales. Most approaches resort to heuristics without formal optimality guarantees. This paper contributes to the computational science of complex systems by introducing a novel MaxSAT-based framework that\xa0can compute optimal network modularity values for larger networks\xa0than previously possible. Leveraging this new capability, we extensively evaluate heuristic solutions and, for the first time, include\xa0the state-of-the-art memetic graph clustering heuristic VieClus. Remarkably, VieClus identifies optimal modularity values for all tested networks, ranging from 103 previously studied instances to 52 new, larger ones, and does so in seconds.\xa0This result contrasts with earlier conclusions that heuristics frequently fail to find the optimal modularity. By combining a powerful MaxSAT encoding, which supports\xa0proof logging for verification, with a fast and effective heuristic,\xa0we demonstrate that even intricate network structures can be tackled efficiently. This synergy brings us closer to making complex network analysis and community detection tractable, robust,\xa0and verifiable—a goal firmly aligned with the core mission\xa0of computational science.}
}


@inproceedings{DBLP:conf/iccS/ZhangJFZL25,
	author = {Taiyao Zhang and
                  Dongzheng Jia and
                  Xingyu Fu and
                  Zhihao Zhang and
                  Qingyun Liu},
	editor = {Michael H. Lees and
                  Wentong Cai and
                  Siew Ann Cheong and
                  Yi Su and
                  David Abramson and
                  Jack J. Dongarra and
                  Peter M. A. Sloot},
	title = {WebAA: Website Association Analysis via Multi-resource Similarity
                  Computation},
	booktitle = {Computational Science - {ICCS} 2025 - 25th International Conference,
                  Singapore, July 7-9, 2025, Proceedings, Part {II}},
	series = {Lecture Notes in Computer Science},
	volume = {15904},
	pages = {50--64},
	publisher = {Springer},
	year = {2025},
	url = {https://doi.org/10.1007/978-3-031-97629-2\_4},
	doi = {10.1007/978-3-031-97629-2\_4},
	timestamp = {Tue, 05 Aug 2025 22:39:49 +0200},
	biburl = {https://dblp.org/rec/conf/iccS/ZhangJFZL25.bib},
	bibsource = {dblp computer science bibliography, https://dblp.org},
	abstract = {The rapid proliferation of websites has posed significant challenges to cyberspace management. To effectively manage websites, we introduce the innovative concept of website association in\xa0this paper and propose a website association model, WebAA. Website association seeks to determine whether target websites belong to\xa0the same organization based on their features, playing a crucial role\xa0in cyberspace management. Unlike website identification, website association enables more accurate organizational alignment through fine-grained analysis of website features. Specifically, malicious website association helps investigators identify the organizations behind them, thereby addressing the root causes of the harm\xa0they cause. In simple terms, the proposed website association model\xa0in this paper consists of four modules. In the first module,\xa0we calculate the similarity score based on external resources\xa0by analyzing the dependency relationships of the target websites\xa0on these resources. The second module employs the BERT model to analyze the similarity of HTML texts between the target websites\xa0and generates a similarity score. The third module focuses on\xa0the website domain names, calculating their similarity using Levenshtein distance and obtaining the similarity score. In the fourth module, the final similarity score is obtained by weighting the\xa0three similarity scores, which is then used to determine whether the\xa0two websites belong to the same organization. Extensive experiments\xa0on two real-world datasets demonstrate that our model can efficiently associate thousands of website pairs within milliseconds with\xa0an accuracy exceeding 90%. Striving for applicability\xa0and replicability, we release ready-to-use raw data from our study.}
}


@inproceedings{DBLP:conf/iccS/Huynh25,
