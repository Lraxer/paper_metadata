                  Cl{\'{e}}lia de Mulatier and
                  Maciej Paszynski and
                  Valeria V. Krzhizhanovskaya and
                  Jack J. Dongarra and
                  Peter M. A. Sloot},
	title = {Dynamic Data Replication for Short Time-to-Completion in a Data Grid},
	booktitle = {Computational Science - {ICCS} 2023 - 23rd International Conference,
                  Prague, Czech Republic, July 3-5, 2023, Proceedings, Part {III}},
	series = {Lecture Notes in Computer Science},
	volume = {14075},
	pages = {668--675},
	publisher = {Springer},
	year = {2023},
	url = {https://doi.org/10.1007/978-3-031-36024-4\_52},
	doi = {10.1007/978-3-031-36024-4\_52},
	timestamp = {Sat, 05 Aug 2023 00:01:41 +0200},
	biburl = {https://dblp.org/rec/conf/iccS/VamosiS23.bib},
	bibsource = {dblp computer science bibliography, https://dblp.org},
	abstract = {Science collaborations use computer grids to run expensive computational tasks on large data sets. Tasks as jobs across the network demand data and thereby workload management and data allocation to maintain the computational workflow. Data allocation includes data placement with different replication factors (multiplicity) of data. The proposed data replication & allocation model can place multitudes of subsets of a data population in a distributed system, such as a computer cluster or computer grid. A stochastic simulation with a data and computing example from the ATLAS Physics Collaboration shows its potential usability in one of the largest Computing Grids. This paper showcases data allocation with different replica factors and various numbers of subsets to improve the overall situation in a computer network.}
}


@inproceedings{DBLP:conf/iccS/KarpioL23,
	author = {Krzysztof Karpio and
                  Piotr Lukasiewicz},
	editor = {Jir{\'{\i}} Mikyska and
                  Cl{\'{e}}lia de Mulatier and
                  Maciej Paszynski and
                  Valeria V. Krzhizhanovskaya and
                  Jack J. Dongarra and
                  Peter M. A. Sloot},
	title = {Detection of Anomalous Days in Energy Demand Using Leading Point Multi-regression
                  Model},
	booktitle = {Computational Science - {ICCS} 2023 - 23rd International Conference,
                  Prague, Czech Republic, July 3-5, 2023, Proceedings, Part {III}},
	series = {Lecture Notes in Computer Science},
	volume = {14075},
	pages = {676--684},
	publisher = {Springer},
	year = {2023},
	url = {https://doi.org/10.1007/978-3-031-36024-4\_53},
	doi = {10.1007/978-3-031-36024-4\_53},
	timestamp = {Sat, 05 Aug 2023 00:01:41 +0200},
	biburl = {https://dblp.org/rec/conf/iccS/KarpioL23.bib},
	bibsource = {dblp computer science bibliography, https://dblp.org},
	abstract = {Leading Point Multi-Regression Model was utilized to detect days with abnormal energy consumption profiles. They were identified based on the statistical analysis of relative errors of the model. Two ranges of error values were identified: above 4.98% and 3.88%–4.98%. All days with anomalous energy consumption profiles were identified as major religious holidays in Poland: Easter, All Saints, and Christmas Eve, as well as days related to a celebration of the new year: New Year’s Eve and New Year.}
}
