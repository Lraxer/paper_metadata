	title = {Allocation of Distributed Resources with Group Dependencies and Availability
                  Uncertainties},
	booktitle = {Computational Science - {ICCS} 2023 - 23rd International Conference,
                  Prague, Czech Republic, July 3-5, 2023, Proceedings, Part {V}},
	series = {Lecture Notes in Computer Science},
	volume = {14077},
	pages = {606--620},
	publisher = {Springer},
	year = {2023},
	url = {https://doi.org/10.1007/978-3-031-36030-5\_48},
