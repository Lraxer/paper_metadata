	title = {Prediction of Casting Mechanical Parameters Based on Direct Microstructure
                  Image Analysis Using Deep Neural Network and Graphite Forms Classification},
	booktitle = {Computational Science - {ICCS} 2023 - 23rd International Conference,
                  Prague, Czech Republic, July 3-5, 2023, Proceedings, Part {V}},
	series = {Lecture Notes in Computer Science},
	volume = {14077},
