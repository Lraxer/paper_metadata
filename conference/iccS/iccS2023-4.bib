                  Valeria V. Krzhizhanovskaya and
                  Jack J. Dongarra and
                  Peter M. A. Sloot},
	title = {3D Tracking of Multiple Drones Based on Particle Swarm Optimization},
	booktitle = {Computational Science - {ICCS} 2023 - 23rd International Conference,
                  Prague, Czech Republic, July 3-5, 2023, Proceedings, Part {IV}},
	series = {Lecture Notes in Computer Science},
	volume = {14076},
	pages = {245--258},
	publisher = {Springer},
	year = {2023},
	url = {https://doi.org/10.1007/978-3-031-36027-5\_18},
	doi = {10.1007/978-3-031-36027-5\_18},
	timestamp = {Sat, 05 Aug 2023 00:01:41 +0200},
	biburl = {https://dblp.org/rec/conf/iccS/KrzeszowskiSZWR23.bib},
	bibsource = {dblp computer science bibliography, https://dblp.org},
	abstract = {This paper presents a method for the tracking of multiple drones in three-dimensional space based on data from a multi-camera system. It uses the Particle Swarm Optimization (PSO) algorithm and methods for background/foreground detection. In order to evaluate the developed tracking algorithm, the dataset consisting of three simulation sequences and two real ones was prepared. The sequences contain from one to ten drones moving with different flight patterns. The simulation sequences were created using the Unreal Engine and the AirSim plugin, whereas the real sequences were registered in the Human Motion Lab at the Polish-Japanese Academy of Information Technology. The lab is equipped with the Vicon motion capture system, which was used to acquire ground truth data. The conducted experiments show the high efficiency and accuracy of the proposed method. For the simulation data, tracking errors from 0.086\xa0m to 0.197\xa0m were obtained, while for real data, the error was 0.101–0.124\xa0m. The system was developed for augmented reality applications, especially games. The dataset is available at http://bytom.pja.edu.pl/drones/.}
}


@inproceedings{DBLP:conf/iccS/GrycukS23,
	author = {Rafal Grycuk and
