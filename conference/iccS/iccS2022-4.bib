	title = {Quantum Approaches for WCET-Related Optimization Problems},
	booktitle = {Computational Science - {ICCS} 2022 - 22nd International Conference,
                  London, UK, June 21-23, 2022, Proceedings, Part {IV}},
	series = {Lecture Notes in Computer Science},
	volume = {13353},
	pages = {202--217},
	publisher = {Springer},
	year = {2022},
