                  Peter M. A. Sloot},
	title = {Scheduling with Multiple Dispatch Rules: {A} Quantum Computing Approach},
	booktitle = {Computational Science - {ICCS} 2022 - 22nd International Conference,
                  London, UK, June 21-23, 2022, Proceedings, Part {IV}},
	series = {Lecture Notes in Computer Science},
	volume = {13353},
