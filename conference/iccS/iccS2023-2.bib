@inproceedings{DBLP:conf/iccS/NieblerSTS23,
	author = {Stefan Niebler and
                  Bertil Schmidt and
                  Holger Tost and
                  Peter Spichtinger},
	editor = {Jir{\'{\i}} Mikyska and
                  Cl{\'{e}}lia de Mulatier and
                  Maciej Paszynski and
                  Valeria V. Krzhizhanovskaya and
                  Jack J. Dongarra and
                  Peter M. A. Sloot},
	title = {Automated Identification and Location of Three Dimensional Atmospheric
                  Frontal Systems},
	booktitle = {Computational Science - {ICCS} 2023 - 23rd International Conference,
                  Prague, Czech Republic, July 3-5, 2023, Proceedings, Part {II}},
	series = {Lecture Notes in Computer Science},
	volume = {14074},
	pages = {3--17},
	publisher = {Springer},
	year = {2023},
	url = {https://doi.org/10.1007/978-3-031-36021-3\_1},
	doi = {10.1007/978-3-031-36021-3\_1},
	timestamp = {Fri, 07 Jul 2023 23:30:22 +0200},
	biburl = {https://dblp.org/rec/conf/iccS/NieblerSTS23.bib},
	bibsource = {dblp computer science bibliography, https://dblp.org},
	abstract = {We present a novel method to identify and locate weather fronts at various pressure levels to create a three dimensional structure using weather data located at the North Atlantic. It provides statistical evaluations regarding the slope and weather phenomena correlated to the identified three dimensional structure. Our approach is based on a deep neural network to locate 2D surface fronts first, which are then used as an initialization to extend them to various height levels. We show that our method is able to detect frontal locations between 500\xa0hPa and 1000\xa0hPa.}
}


@inproceedings{DBLP:conf/iccS/KasztelnikNMMNGZMB23,
	author = {Marek Kasztelnik and
                  Piotr Nowakowski and
                  Jan Meizner and
                  Maciej Malawski and
                  Adam Nowak and
                  Krzysztof Gadek and
                  Karol Zajac and
                  Antonino Amedeo La Mattina and
                  Marian Bubak},
	editor = {Jir{\'{\i}} Mikyska and
                  Cl{\'{e}}lia de Mulatier and
                  Maciej Paszynski and
                  Valeria V. Krzhizhanovskaya and
                  Jack J. Dongarra and
                  Peter M. A. Sloot},
	title = {Digital Twin Simulation Development and Execution on {HPC} Infrastructures},
	booktitle = {Computational Science - {ICCS} 2023 - 23rd International Conference,
                  Prague, Czech Republic, July 3-5, 2023, Proceedings, Part {II}},
	series = {Lecture Notes in Computer Science},
	volume = {14074},
	pages = {18--32},
	publisher = {Springer},
	year = {2023},
	url = {https://doi.org/10.1007/978-3-031-36021-3\_2},
	doi = {10.1007/978-3-031-36021-3\_2},
	timestamp = {Fri, 07 Jul 2023 23:30:22 +0200},
	biburl = {https://dblp.org/rec/conf/iccS/KasztelnikNMMNGZMB23.bib},
	bibsource = {dblp computer science bibliography, https://dblp.org},
	abstract = {The Digital Twin paradigm in medical care has recently gained popularity among proponents of translational medicine, to enable clinicians to make informed choices regarding treatment on the basis of digital simulations. In this paper we present an overview of functional and non-functional requirements related to specific IT solutions which enable such simulations - including the need to ensure repeatability and traceability of results - and propose an architecture that satisfies these requirements. We then describe a computational platform that facilitates digital twin simulations, and validate our approach in the context of a real-life medical use case: the BoneStrength application.}
}


@inproceedings{DBLP:conf/iccS/TakahashiGTYAT23,
	author = {Naoya Takahashi and
                  Ritsuka Gomi and
                  Ayato Takii and
                  Masashi Yamakawa and
                  Shinichi Asao and
                  Seiichi Takeuchi},
	editor = {Jir{\'{\i}} Mikyska and
                  Cl{\'{e}}lia de Mulatier and
                  Maciej Paszynski and
                  Valeria V. Krzhizhanovskaya and
                  Jack J. Dongarra and
                  Peter M. A. Sloot},
	title = {Numerical Simulation of the Octorotor Flying Car in Sudden Rotor Stop},
	booktitle = {Computational Science - {ICCS} 2023 - 23rd International Conference,
                  Prague, Czech Republic, July 3-5, 2023, Proceedings, Part {II}},
	series = {Lecture Notes in Computer Science},
	volume = {14074},
	pages = {33--46},
	publisher = {Springer},
	year = {2023},
	url = {https://doi.org/10.1007/978-3-031-36021-3\_3},
	doi = {10.1007/978-3-031-36021-3\_3},
	timestamp = {Wed, 05 Jul 2023 16:00:03 +0200},
	biburl = {https://dblp.org/rec/conf/iccS/TakahashiGTYAT23.bib},
	bibsource = {dblp computer science bibliography, https://dblp.org},
	abstract = {Currently, manned drones, also known as flying cars, are attracting attention, but due to legal restrictions and fear of accidents, it is difficult to fly them. We therefore present a numerical simulation of the sudden stop of the rotor of an octrotor flying car. In this paper, we consider the interaction between fluid and rigid-body in a 6-degrees of freedom flight simulation of a flying car. For the purpose, the attitude of the aircraft is determined based on the force generated from the flow field around the aircraft due to the rotation of the rotor. The motion of the aircraft is obtained from the equations of motion of translation and rotation, and Newton's equation of motion and Euler's equation of rotation are used. A multi-axis sliding mesh is adopted for the rotation of the rotor, and calculations with multiple rotating bodies in the computational grid are performed. In addition, we use the motion computational domain (MCD) method to represent the free motion of the octrotor flying car by the motion computational domain itself. Using the above method, we will show the appropriate rotation method from various rotor stop patterns, demonstrate the safety of the octrotor flying car, and clarify the behavior of the aircraft and the surrounding flow field.}
}


@inproceedings{DBLP:conf/iccS/BesozziBG23,
