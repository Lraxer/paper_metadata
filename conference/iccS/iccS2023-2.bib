                  Cl{\'{e}}lia de Mulatier and
                  Maciej Paszynski and
                  Valeria V. Krzhizhanovskaya and
                  Jack J. Dongarra and
                  Peter M. A. Sloot},
	title = {Chemical Mixing Simulations with Integrated {AI} Accelerator},
	booktitle = {Computational Science - {ICCS} 2023 - 23rd International Conference,
                  Prague, Czech Republic, July 3-5, 2023, Proceedings, Part {II}},
	series = {Lecture Notes in Computer Science},
	volume = {14074},
	pages = {494--508},
	publisher = {Springer},
	year = {2023},
	url = {https://doi.org/10.1007/978-3-031-36021-3\_50},
	doi = {10.1007/978-3-031-36021-3\_50},
	timestamp = {Fri, 07 Jul 2023 23:30:22 +0200},
	biburl = {https://dblp.org/rec/conf/iccS/RojekWG23.bib},
	bibsource = {dblp computer science bibliography, https://dblp.org},
	abstract = {In this work, we develop a method for integrating an AI model with a CFD solver to predict chemical mixing simulations’ output. The proposed AI model is based on a deep neural network with a variational autoencoder that is managed by our AI supervisor. We demonstrate that the developed method allows us to accurately accelerate the steady-state simulations of chemical reactions performed with the MixIT solver from Tridiagonal solutions. In this paper, we investigate the accuracy and performance of AI-accelerated simulations, considering three different scenarios: i) prediction in cases with the same geometry of mesh as used during training the model, ii) with a modified geometry of tube in which the ingredients are mixed, iii) with a modified geometry of impeller used to mix the ingredients. Our AI model is trained on a dataset containing 1500 samples of simulated scenarios and can accurately predict the process of chemical mixing under various conditions. We demonstrate that the proposed method achieves accuracy exceeding 90% and reduces the execution time up to 9 times.}
}


@inproceedings{DBLP:conf/iccS/UriarteTPRV23,
	author = {Carlos Uriarte and
                  Jamie M. Taylor and
                  David Pardo and
