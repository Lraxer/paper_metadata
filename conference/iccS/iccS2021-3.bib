	biburl = {https://dblp.org/rec/conf/iccS/HermansMT21.bib},
	bibsource = {dblp computer science bibliography, https://dblp.org},
	abstract = {The interactions between a person and his or her primary caregiver shape the attachment pattern blueprint of how this person behaves in intimate relationships later in life. This attachment pattern has a lifelong effect on an individual, but also evolves throughout a person's life. In this paper, an adaptive network was designed and simulated to provide insights into how an attachment pattern is created and how this pattern then has its effects and evolves as the person develops new intimate relationships at older age.}
}


@inproceedings{DBLP:conf/iccS/PavlovskiiDK21,
