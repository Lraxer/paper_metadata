@inproceedings{DBLP:conf/iccS/SzczepanskiCPP21,
	author = {Mateusz Szczepanski and
                  Michal Choras and
                  Marek Pawlicki and
                  Aleksandra Pawlicka},
	editor = {Maciej Paszynski and
                  Dieter Kranzlm{\"{u}}ller and
                  Valeria V. Krzhizhanovskaya and
                  Jack J. Dongarra and
                  Peter M. A. Sloot},
	title = {The Methods and Approaches of Explainable Artificial Intelligence},
	booktitle = {Computational Science - {ICCS} 2021 - 21st International Conference,
                  Krakow, Poland, June 16-18, 2021, Proceedings, Part {IV}},
	series = {Lecture Notes in Computer Science},
	volume = {12745},
	pages = {3--17},
	publisher = {Springer},
	year = {2021},
	url = {https://doi.org/10.1007/978-3-030-77970-2\_1},
	doi = {10.1007/978-3-030-77970-2\_1},
	timestamp = {Mon, 03 Jan 2022 22:23:22 +0100},
	biburl = {https://dblp.org/rec/conf/iccS/SzczepanskiCPP21.bib},
	bibsource = {dblp computer science bibliography, https://dblp.org},
	abstract = {Artificial Intelligence has found innumerable applications, becoming ubiquitous in the contemporary society. From making unnoticeable, minor choices to determining people’s fates (the case of predictive policing). This fact raises serious concerns about the lack of explainability of those systems. Finding ways to enable humans to comprehend the results provided by AI is a blooming area of research right now. This paper explores the current findings in the field of Explainable Artificial Intelligence (xAI), along with xAI methods and solutions that realise them. The paper provides an umbrella perspective on available xAI options, sorting them into a range of levels of abstraction, starting from community-developed code snippets implementing facets of xAI research all the way up\xa0to comprehensive solutions utilising state-of-the-art achievements in the domain.}
}


@inproceedings{DBLP:conf/iccS/TarczewskaMG21,
	author = {Martyna Tarczewska and
                  Anna Marciniak and
                  Agata Gielczyk},
	editor = {Maciej Paszynski and
                  Dieter Kranzlm{\"{u}}ller and
                  Valeria V. Krzhizhanovskaya and
                  Jack J. Dongarra and
                  Peter M. A. Sloot},
	title = {Fake or Real? The Novel Approach to Detecting Online Disinformation
                  Based on Multi {ML} Classifiers},
	booktitle = {Computational Science - {ICCS} 2021 - 21st International Conference,
                  Krakow, Poland, June 16-18, 2021, Proceedings, Part {IV}},
	series = {Lecture Notes in Computer Science},
	volume = {12745},
	pages = {18--27},
	publisher = {Springer},
	year = {2021},
	url = {https://doi.org/10.1007/978-3-030-77970-2\_2},
	doi = {10.1007/978-3-030-77970-2\_2},
	timestamp = {Tue, 15 Jun 2021 17:23:43 +0200},
	biburl = {https://dblp.org/rec/conf/iccS/TarczewskaMG21.bib},
	bibsource = {dblp computer science bibliography, https://dblp.org},
	abstract = {Background: the machine learning (ML) techniques have been implemented in numerous applications and domains, including health-care, security, entertainment, and sports. This paper presents how ML can be used for detecting fake news. The problem of online disinformation has recently become one of the most challenging issues of computer science. Methods: in this research, a fake news detection method based on multi classifiers (CNN, XGBoost, Random Forest, Naive Bayes, SVM) has been developed. In the proposed method, two classifiers cooperate; consequently, they obtain better results. Realistic, publicly available data was used in order to train and test the classifiers, Results: in the article, several experiments were presented; they differ in the implemented classifiers, and some improved parameters. Promising results (accuracy\xa0=\xa00.95, precision\xa0=\xa00.99, recall\xa0=\xa00.91, and F1-score\xa0=\xa00.95) were reported. Conclusion: the presented research proves that machine learning is a promising approach to fake news detection.}
}


@inproceedings{DBLP:conf/iccS/KulaKC021,
	author = {Sebastian Kula and
                  Rafal Kozik and
                  Michal Choras and
